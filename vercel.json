{
    "version": 2,
    "builds": [
      {
        "src": "public/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_KEY": "",
      "APP_URL": "",
      "LOG_CHANNEL": "stderr",
      "CACHE_DRIVER": "array",
      "SESSION_DRIVER": "cookie",
      "QUEUE_DRIVER": "sync"
    },
    "functions": {
      "api/*": {
        "runtime": "vercel-php@0.5.2"
      }
    }
  }
  