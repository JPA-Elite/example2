{
    "version": 2,
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.5.2"
      }
    },
    "routes": [{
      "src": "/(.*)",
      "dest": "/api/index.php"
    }],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "APP_URL": "",
      "VIEW_COMPILED_PATH": "/tmp",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie"
    }
  }
  